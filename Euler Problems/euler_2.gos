declare sum = 0; 
declare i = 0; 
declare j = 1; 
declare k = 0; 
while j<=4000000 do {
    assign k = i + j; 
    assign i = j; 
    assign j = k; 
    if j%2==0 then assign sum = sum + j; 
    }; 

print(sum);




deffunct fibo(g){
    if g<=1 then functret(0);
    else {functret (callfun fibo(g-1);+callfun fibo(g-2););};
};

print(callfun fibo(4););